title GIT 代码提交批处理 
color 16
@echo off
echo *************************************************************
echo *                GIT 代码提交批处理                         
echo *************************************************************
echo;
echo;
echo 开始提交代码到本地仓库
echo 当前目录是：%cd%
echo;
echo;
echo 开始识别当前git的版本
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git --version
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;
echo;
echo 开始添加变更
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git add -A .
echo 执行结束！
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;
echo 提交变更到本地仓库
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git commit -m "修改代码"
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;
echo 将变更情况提交到远程git服务器
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git push origin master
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;
echo 下载远程仓
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git clone https://github.com/wodailicom/wodaili.com.github.io.git
echo ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;
echo 批处理执行完毕！
echo;
pause